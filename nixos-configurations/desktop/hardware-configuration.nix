  fileSystems."/home" = {
    device = "/dev/disk/by-uuid/d9a88f45-ab4a-4f43-95de-524d8acea478";
    fsType = "btrfs";
    options = [ "subvol=@home" ];
  };

  fileSystems."/games" = {
    device = "/dev/disk/by-uuid/d9a88f45-ab4a-4f43-95de-524d8acea478";
