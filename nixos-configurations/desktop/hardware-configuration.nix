  };

  fileSystems."/var/log" = {
    device = "/dev/disk/by-uuid/d9a88f45-ab4a-4f43-95de-524d8acea478";
    fsType = "btrfs";
    options = [ "subvol=@log" ];
  };

  fileSystems."/tmp" = {
    device = "/dev/disk/by-uuid/d9a88f45-ab4a-4f43-95de-524d8acea478";
    fsType = "btrfs";
    options = [ "subvol=@tmp" ];
  };

  fileSystems."/var/cache" = {
